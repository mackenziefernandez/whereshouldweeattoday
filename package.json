{
  "name": "kitchen-kalendar",
  "version": "2.0.0",
  "description": "Meal planner, grocery list compiler and recipe storage",
  "main": "index.js",
  "repository": "git@github.com:mfernandez-rally/kitchen-kalendar.git",
  "scripts": {
    "build": "npm run build:client && npm run build --prefix functions",
    "build:client": "rm -rf dist && webpack --config ./config/webpack.config.prod.js --bail",
    "deploy:test": "NODE_ENV=test npm run build && firebase use kitchen-kalendar-test && firebase deploy",
    "lint": "eslint src test && tslint \"{./**/*.ts,./**/*.tsx}\" --exclude \"{./node_modules/**,./**/*.d.ts}\"",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "test": "jest --config jest.config.json",
    "test:ci": "npm run lint && npm run test",
    "test:watch": "jest --config jest.config.json --watch"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.5",
    "duckbase": "^0.3.0",
    "firebase": "^4.3.1",
    "history": "^4.7.2",
    "material-ui": "^0.19.2",
    "material-ui-color-picker": "^1.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-linkify": "^0.2.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^2.0.1",
    "recipe-ingredient-parser": "^1.5.3",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/history": "^4.6.0",
    "@types/jest": "^20.0.8",
    "@types/material-ui": "^0.18.1",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.9",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "firebase-tools": "^3.12.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.1.0",
    "node-sass": "^4.5.3",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "engines": {
    "node": "8.9.4"
  }
}
